    <template title="tinhte_xentag_hook_admin_forum_edit_panes"><![CDATA[<li>
	<xen:radiounit label="{xen:phrase option_Tinhte_XenTag_requiresTag}" name="tinhte_xentag_options[requiresTag]" value="{$forum.tinhte_xentag_options.requiresTag}" explain="{xen:phrase tinhte_xentag_forum_requires_tag_explain}">
		<xen:option value="">{xen:phrase default} ({xen:if '{xen:helper Tinhte_XenTag_getOption, requiresTag} > 0', '{xen:phrase yes}', '{xen:phrase no}'})</xen:option>
		<xen:option value="1">{xen:phrase yes}</xen:option>
		<xen:option value="0">{xen:phrase no}</xen:option>
	</xen:radiounit>
	
	<xen:radiounit label="{xen:phrase option_Tinhte_XenTag_maximumTags}" name="tinhte_xentag_options[maximumTags]" explain="{xen:phrase tinhte_xentag_forum_maximum_tags_explain}">
		<xen:option value="" selected="{$forum.tinhte_xentag_options.maximumTags} == ''">{xen:phrase default} ({xen:if '{xen:helper Tinhte_XenTag_getOption, maximumTags} > 0', '{xen:helper Tinhte_XenTag_getOption, maximumTags}', '{xen:phrase unlimited}'})</xen:option>
		<xen:option value="other" selected="{$forum.tinhte_xentag_options.maximumTags} != ''" inputclass="Disabler">{xen:phrase other}</xen:option>
		<xen:html>
			<div id="ctrl_tinhte_xentag_optionsmaximumTags_other_Disabler">
				<xen:spinbox name="tinhte_xentag_options_maximumTags_value" value="{xen:calc '{$forum.tinhte_xentag_options.maximumTags} + 0'}" inputclass="OptOut" />
			</div>
		</xen:html>
	</xen:radiounit>
</li>]]></template>
