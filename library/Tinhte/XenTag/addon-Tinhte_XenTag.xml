<xen:h1>{xen:phrase tinhte_xentag_tag_confirm_deletion}</xen:h1>

<xen:require css="delete_confirmation.css" />

<xen:form action="{xen:adminlink 'xentag-tags/delete-empty-tags'}" class="deleteConfirmForm formOverlay">

	<p>{xen:phrase tinhte_xentag_please_confirm_delete_empty_tags}</p>

